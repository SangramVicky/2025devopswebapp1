pipeline {
    agent any
    stages {
        stage('GIT') {
            steps {
                git credentialsId: 'GIT', url: 'https://github.com/SangramVicky/2025devopswebapp1'
            }
        }
        stage('Build') {
            steps {
                    script {
                        def mvnHome = tool name: 'maven3.x', type: 'maven'
                        sh "${mvnHome}/bin/mvn clean package"
                    }
                }
            }
        }
    }
